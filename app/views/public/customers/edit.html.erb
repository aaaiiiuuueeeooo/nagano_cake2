<h6>会員情報編集</h6>
<%= form_with model: @customer,url: customer_path, method: :patch, local: true do |form| %>
<div class= "row field">
  <div class="col-md-4"><%= form.label :name, "氏名" %></div> <div class="col-md-8"><%= form.text_field :last_name %></div> <div><%= form.text_field :first_name %></div>
</div>
<div class= "row field">
  <div class="col-md-4"><%= form.label :name_kana, "フリガナ" %></div> <div class="col-md-8"><%= form.text_field :last_name_kana %></div> <div><%= form.text_field :first_name_kana %></div>
</div><div class= "row field">
  <div class="col-md-4"><%= form.label :postal_code, "郵便番号" %></div> <div class="col-md-8"><%= form.text_field :postal_code %></div>
</div><div class= "row field">
  <div class="col-md-4"><%= form.label :address, "住所" %></div> <div class="col-md-8"><%= form.text_field :address %></div>
</div><div class= "row field">
  <div class="col-md-4"><%= form.label :telephone_number, "電話番号" %></div> <div class="col-md-8"><%= form.text_field :telephone_number %></div>
</div><div class= "row field">
  <div class="col-md-4"><%= form.label :email, "メールアドレス" %></div> <div class="col-md-8"><%= form.text_field :email %></div>
</div>
<div>
  <%= form.submit '変更を保存' %>
</div>
<% end %>
<%= link_to "退会する",customers_unsubscribe_path %>