<h3>配送先編集</h3>
<%= form_with model: @addresses,url: addresses_path, method: :patch, local: true do |form| %>
<div><%= "郵便番号（ハイフンなし）" %></div> <div><%= form.text_field :postal_code,class:"form-control" %></div>
<div><%= "住所" %></div> <div><%= form.text_field :address,class:"form-control" %></div>
<div><%= "宛名" %></div> <div><%= form.text_field :name,class:"form-control" %></div>
<div><%= form.submit '変更内容を保存' %></div>
<% end %>